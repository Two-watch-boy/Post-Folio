<p id="notice"><%= notice %></p>

<h1>Listing Projects</h1>

  <body>
    <div class="container">
      <div class="row">
        <% @projects.each do |project| %>
          <div class="col-sm-3">
            <%= link_to (image_tag project.thumbnail_url(:display)), project, class:"thumbnail" %>
            <p class="thumbTitle"><%= link_to (project.title), project %></p>
            </br>
            </br>
          </div>
        <% end %>
      </div>
    </div>
  </body>

<br>

