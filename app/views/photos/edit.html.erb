<h1>Editing Photo</h1>

<%= form_for [@project, @photo] do |f| %>
<div>
  <%= f.text_field :title, placeholder: "Title" , class: "newUser form-control"%>
</div>
<div>
  <%= f.file_field :image,class: "newUser form-control userPicture"%>
</div>
<div class="actions">
  <%= f.submit %>
</div>
<% end %>


<%= link_to 'Back', project_photos_path %>
